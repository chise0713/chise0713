#!/bin/bash
alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'
alias grep='grep --color=auto'
alias l.='ls -A'
alias ll='ls -AlhF'
alias ls='ls --color=auto'
alias jcat='journalctl -o cat -f -u'
alias free='free -h'
alias proton-call='proton-call -c "/usr/share/steam/compatibilitytools.d/proton/"'
alias soft-reboot='systemctl soft-reboot'
alias omb='cd /usr/local/share/oh-my-bash/'
